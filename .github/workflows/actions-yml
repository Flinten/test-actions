name: Action workflow

on: [push]

jobs: 
  run-github-actions:
    runs-on: ubuntu-latest
    steps:
      - name: Simple JS action
        id: greet
        uses: actions/hello-world-javascript-action@v1
        with:
          who-to-great: John
      - name: Log greeting Time
        run: echo "${{steps.greet.time}}"  
       